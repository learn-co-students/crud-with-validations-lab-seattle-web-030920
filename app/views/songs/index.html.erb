<table style="width:100%">
    <thead>
    <tr>
        <th>Title</th>        
        <th>Release Year</th>
        <th>Artist Name</th>
        <th>Genre</th>
    </tr>
    </thead>
    <tbody>
    <%= @songs.each {|song|%>
        <tr>
            <td>song.title</td>    
            <td>song.release_year</td>
            <td>song.artist_name</td>
            <td>song.genre</td>
        
            <td>
            <%=link_to 'View', song %>
            <%=link_to 'Edit', edit_song_path(song)  %>
            <%=link_to 'Delete', song, method: :delete, data: {confirm: 'Delete song, are you sure?' }  %>
            </td>
        <tr>    
    </tbody>
    <% } %>
</table>